<body style="background-image: url(img/nmamit.jpeg);background-size: cover;background-repeat: no-repeat;">
    <div id="liveAlertPlaceholder"></div>
    <div class="container" id="content01">
        <!-- <div class="img-slider">
            <h2>Welcome to Noodle</h2>
        </div> -->

        <div class="content">
            <h1 style="font-size: 39px;padding-left: 81px;padding-top: 18px;">Get Started</h1>
            <p>Already have account ? <a href="/signin" id="login">Sign In </a></p>
            <form id="form" method="POST">
                <label for="name" id="nlabel">Name</label> <br>
                <input type="text" id="name" name="name" placeholder="Amogha Mayya" autocomplete="off" required> <br>

                <label for="email" id="elabel">Email</label> <br>
                <input type="email" id="email" name="email" placeholder="amoghamayya@gmail.com" autocomplete="off"
                    required> <br>

                <label for="usn" id="nlabel">USN</label> <br>
                <input type="text" id="usn" name="usn" placeholder="4nm19cs019" autocomplete="off" required> <br>

                <label for="password" id="plabel">Password</label> <br>
                <input type="password" name="password" id="password"> <br>
                <button type="submit" onclick="UserRegister()" class="btn btn-primary" id="btn13">Sign
                    Up</button>
            </form>
        </div>
    </div>
    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.1.3/dist/js/bootstrap.bundle.min.js"
