                <div class="card-header" style="font-size: larger;font-weight: bold; background-color: #faeba9;">
                    <%= events.title %>
                </div>
                <div class="card-body" style="background-color: #f8dec4;">
                    <p class="card-text">
                        <%= events.description %>
                    </p>
                </div>
            </div>
            <% }) %>



                <div class="container px-5 py-20 mx-auto">
